<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Netwide Assembler"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="NASM"
			DisplayName="Netwide Assembler"
			CommandLine="nasmw.exe -f win32  [AllOptions] [AdditionalOptions] [Inputs]"
			Outputs="[$ObjectFileName]"
			FileExtensions="*.asm"
			ExecutionDescription="Assembling..."
			>
			<Properties>
				<StringProperty
					Name="ObjectFileName"
					DisplayName="Object File Name"
					PropertyPageName="Object File"
					Description="Specifies the name of the output object file.     (-o[file])"
					Switch="-o&quot;[value]&quot;"
					DefaultValue="$(IntDir)\$(InputName).obj"
				/>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
